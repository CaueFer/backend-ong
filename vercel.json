{
  "version": 2,
  "builds": [
    {
      "src": "/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "rewrites": [
    { "source": "/api-docs", "destination": "/api-docs/index.html" }
  ],
  "headers": [
    {
      "source": "/api-docs/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}